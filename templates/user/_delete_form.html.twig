<form method="post" action="{{ path('app_user_delete', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" id="formDelete">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
	<button id="btnDeleteProfil">
		<i class="bi bi-trash-fill" id="iconDeleteProfil"></i>
	</button>
</form>
